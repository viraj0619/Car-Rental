<script>
    document.addEventListener('DOMContentLoaded', () => {
    const categorySec = document.querySelector('.category-sec');
    let isDown = false;
    let startX;
    let scrollLeft;

    categorySec.addEventListener('mousedown', (e) => {
        isDown = true;
        categorySec.classList.add('active');
        startX = e.pageX - categorySec.offsetLeft;
        scrollLeft = categorySec.scrollLeft;
    });

    categorySec.addEventListener('mouseleave', () => {
        isDown = false;
        categorySec.classList.remove('active');
    });

    categorySec.addEventListener('mouseup', () => {
        isDown = false;
        categorySec.classList.remove('active');
    });

    categorySec.addEventListener('mousemove', (e) => {
        if (!isDown) return;
        e.preventDefault();
        const x = e.pageX - categorySec.offsetLeft;
        const walk = (x - startX) * 2; //scroll-fast
        categorySec.scrollLeft = scrollLeft - walk;
    });
});


